{
  "name": "typescript-errors-explorer",
  "version": "1.0.0",
  "description": "TypeScript errors explorer",
  "source": "src/index.html",
  "type": "module",
  "scripts": {
    "start": "npm run clean && parcel --no-source-maps --no-cache",
    "build": "npm run clean && parcel build --no-source-maps --no-cache",
    "serve": "python3 -m http.server -d dist",
    "clean": "rm -rf dist .parcel-cache",
    "test": "tsc"
  },
  "dependencies": {
    "jsx-dom-runtime": "^0.35.0",
    "storeon-velo": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "mini-css-class-name": "^0.13.2",
    "parcel": "^2.8.3",
    "postcss": "^8.4.23",
    "postcss-modules": "^4.3.1",
    "typescript": "^5.0.4"
  },
  "browser": {
    "engines": {
      "browsers": [
        "> 1%",
        "not dead"
      ]
    }
  },
  "targets": {
    "default": {
      "publicUrl": " /typescript-errors-explorer"
    }
  },
  "author": {
    "name": "Alexander Zaytsev",
    "email": "zaytsev126@gmail.com",
    "url": "https://twitter.com/_shoonia"
  },
  "license": "MIT"
}
