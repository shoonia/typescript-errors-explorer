{
  "name": "typescript-errors-explorer",
  "version": "1.0.0",
  "description": "TypeScript errors explorer",
  "source": "src/index.html",
  "type": "module",
  "scripts": {
    "start": "npm run clean && parcel --no-cache",
    "build": "npm run clean && parcel build --no-cache",
    "serve": "python3 -m http.server -d dist",
    "clean": "rm -rf dist .parcel-cache",
    "test": "tsc"
  },
  "dependencies": {
    "jsx-dom-runtime": "^0.70.2",
    "storeon-velo": "^5.0.0-rc.5"
  },
  "devDependencies": {
    "@types/node": "^20.16.11",
    "parcel": "^2.12.0",
    "postcss": "^8.4.47",
    "postcss-modules": "^4.3.1",
    "typescript": "^5.6.3"
  },
  "browsersList": "> 1%, not dead",
  "targets": {
    "default": {
      "sourceMap": false,
      "publicUrl": " /typescript-errors-explorer/"
    }
  },
  "author": {
    "name": "Alexander Zaytsev",
    "email": "zaytsev126@gmail.com",
    "url": "https://x.com/_shoonia"
  },
  "license": "MIT"
}
